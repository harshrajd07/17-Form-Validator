public class Pallindrome
{
	public static void main(String[] args) 
	{
		int n1=Integer.parseInt(args[0]);
		int r,rev=0;
		int temp=n1;
		while(n1!=0){
		r=n1%10;
		rev=rev*10+r;
		n1=n1/10;
		}
		if(temp==rev)
			{System.out.println("Pallindrome");
		}
		else
			{System.out.println("Not a Pallindrome");
		}
		
	}
}
